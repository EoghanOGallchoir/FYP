@model IEnumerable<FYP.Models.User>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProgressXP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz5)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quiz6)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GroupId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProgressXP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz5)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quiz6)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GroupId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
